{
	"name": "todo-dev",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"start": "nodemon server.js",
		"dev": "npm-run-all -p start serve",
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"lint": "vue-cli-service lint"
	},
	"dependencies": {
		"axios": "^0.18.0",
		"bootstrap": "^4.1.3",
		"json-server": "^0.14.0",
		"vue": "^2.5.17",
		"vuex": "^3.0.1"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "^3.0.3",
		"@vue/cli-plugin-eslint": "^3.0.3",
		"@vue/cli-service": "^3.0.3",
		"nodemon": "^1.18.4",
		"npm-run-all": "^4.1.3",
		"vue-template-compiler": "^2.5.17"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"eslint:recommended"
		],
		"rules": {},
		"parserOptions": {
			"parser": "babel-eslint"
		}
	},
	"postcss": {
		"plugins": {
			"autoprefixer": {}
		}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not ie <= 8"
	]
}
